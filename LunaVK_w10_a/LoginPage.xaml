	<Grid>
		<Grid Background="Black">
			<Grid
				Name="RootBack"
				Background="{ThemeResource SystemAccentColor}"
				Opacity="0" />
			<StackPanel
				Name="_stackPanelCenter"
				MaxWidth="250"
				VerticalAlignment="Center"
				Visibility="{Binding ElementName=_cbProxy, Path=IsChecked, Converter={StaticResource BoolToVisibilityConverter}, ConverterParameter=True}">
				<StackPanel.RenderTransform>
					<TranslateTransform x:Name="tr" />
				</StackPanel.RenderTransform>

				<Image
					x:Name="HeaderImage"
					framework:MetroInMotion.IsTiltEnabled="True"
					Source="/Assets/Img/SplashScreen.png" />

				<Grid
					Name="gridLogin"
					Margin="0 10"
					Opacity="0">
					<Grid.RenderTransform>
						<TranslateTransform x:Name="Transform1" Y="100" />
					</Grid.RenderTransform>
					<uc:IconUC
						HorizontalAlignment="Left"
						FontSize="16"
						Foreground="White"
						Glyph="&#xE77B;" />
					<TextBox
						Name="textBoxUsername"
						x:Uid="UserName"
						Padding="20 5 0 0"
						Background="Transparent"
						BorderThickness="0 0 0 1"
						FontSize="16"
						Foreground="White"
						InputScope="EmailNameOrAddress"
						IsSpellCheckEnabled="False"
						KeyUp="textBoxUsername_KeyUp"
						PlaceholderText="User name"
						TextChanged="textBoxUsername_TextChanged">
						<TextBox.Resources>
							<SolidColorBrush x:Key="TextControlForegroundPointerOver" Color="White" />
							<SolidColorBrush x:Key="TextControlForegroundFocused" Color="White" />
						</TextBox.Resources>
					</TextBox>
				</Grid>

				<Grid Name="gridPass" Opacity="0">
					<Grid.RenderTransform>
						<TranslateTransform x:Name="Transform2" Y="100" />
					</Grid.RenderTransform>
					<uc:IconUC
						HorizontalAlignment="Left"
						FontSize="16"
						Foreground="White"
						Glyph="&#xE72E;" />
					<PasswordBox
						x:Name="passwordBox"
						x:Uid="Password"
						Padding="20 5 0 0"
						Background="Transparent"
						BorderThickness="0 0 0 1"
						Foreground="White"
						KeyUp="passwordBox_KeyUp"
						PasswordChanged="passwordBox_PasswordChanged"
						PlaceholderText="Password">
						<PasswordBox.Resources>
							<SolidColorBrush x:Key="TextControlForegroundPointerOver" Color="White" />
							<SolidColorBrush x:Key="TextControlForegroundFocused" Color="White" />
						</PasswordBox.Resources>
					</PasswordBox>
				</Grid>

				<Grid>
					<Button
						Name="LoginBtn"
						x:Uid="LogIn"
						Margin="0 10"
						HorizontalAlignment="Right"
						Click="Login_Click"
						Content="LogIn"
						IsEnabled="False" />
					<ProgressRing Name="_progressRing" Foreground="White" />
				</Grid>

				<TextBlock
					Name="_error"
					Foreground="Red"
					MaxLines="2"
					Opacity="0"
					TextWrapping="Wrap" />
			</StackPanel>

			<Grid Height="48" VerticalAlignment="Bottom">
				<Grid.ColumnDefinitions>
					<ColumnDefinition />
					<ColumnDefinition />
				</Grid.ColumnDefinitions>
				<TextBlock
					x:Uid="SignUp"
					HorizontalAlignment="Center"
					VerticalAlignment="Center"
					Foreground="White"
					Tapped="TextBlock_Tapped_1"
					Text="SignUp" />
				<TextBlock
					x:Uid="TermsOfUse"
					Grid.Column="1"
					HorizontalAlignment="Center"
					VerticalAlignment="Center"
					Foreground="White"
					Tapped="TextBlock_Tapped"
					Text="TermsOfUse" />
			</Grid>

			<StackPanel
				Name="_stackPanelLeft"
				Margin="20 30"
				HorizontalAlignment="Left"
				VerticalAlignment="Top">
				<Button
					Click="OAuthButton_Click"
					Content="OAuth login"
					Foreground="White" />
				<CheckBox
					Name="_cbProxy"
					Margin="0 5"
					Content="Use proxy"
					Foreground="White" />

				<StackPanel
					MaxWidth="200"
					HorizontalAlignment="Left"
					Visibility="{Binding ElementName=_cbProxy, Path=IsChecked, Converter={StaticResource BoolToVisibilityConverter}}">
					<TextBlock
						FontSize="14"
						Foreground="White"
						Text="Прокси от:"
						TextWrapping="Wrap" />
					<Image Source="/Assets/xtrafrancyz.png" />
				</StackPanel>
			</StackPanel>

			<ProgressRing
				x:Name="progressRing"
				Width="200"
				Height="100"
				HorizontalAlignment="Center"
				VerticalAlignment="Center"
				Foreground="White"
				IsActive="True"
				Opacity="0"
				Style="{StaticResource ThreeBounceProgressRingStyle}"
				Visibility="Collapsed" />

			<WebView
				x:Name="browser"
				NavigationCompleted="browser_NavigationCompleted"
				NavigationFailed="browser_NavigationFailed"
				NavigationStarting="_browser_NavigationStarting"
				Opacity="0"
				Visibility="Collapsed" />
		</Grid>

















