												<SplineDoubleKeyFrame
													KeySpline="0.42,0,0.58,1"
													KeyTime="0:0:1.1"
													Value="0" />
												<SplineDoubleKeyFrame KeyTime="0:0:1.6" Value="0" />
											</DoubleAnimationUsingKeyFrames>
										</Storyboard>
									</VisualState>
								</VisualStateGroup>
							</VisualStateManager.VisualStateGroups>
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

