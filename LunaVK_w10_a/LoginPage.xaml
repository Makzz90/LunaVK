            <Button Content="Войти" Click="Button_Click" HorizontalAlignment="Stretch"  Padding="0" MinHeight="60" Height="60" Style="{StaticResource ButtonStyle}" Foreground="White" VerticalAlignment="Bottom" Margin="80 20" Name="_btnLogin"/>
            <ProgressRing x:Name="progressRing" HorizontalAlignment="Center" VerticalAlignment="Center" IsActive="True" Style="{StaticResource CircleProgressRingStyle}" Foreground="White" Width="120" Height="50" Margin="0,300,0,0" Visibility="Visible" Opacity="1"/>



